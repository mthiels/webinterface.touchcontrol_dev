.semi-trans {
	opacity : 0.8;
}

.showInfo {

    background: #fff;

    .avatar {
        background-position: left center;
        background-repeat: no-repeat;
        border-width: 0 1px 1px 0;
        border: 0;
		width: auto;
		height: 100px;	
    }
}

.x-list-item.castListInfo {
    font-size: 0.5em;
	height: 100px;
}

.x-list-item.listInfo {

    font-size: 0.9em;
//    line-height: 1.1em;

    .playIndex {
        background-position: center top;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: left;
        border: 0;
        width: 32px;
        height: 32px;
        background-size: 32px 32px;
    }
    .playRemove {
        background-position: center top;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: right;
        border: 0;
        width: 32px;
        height: 32px;
        background-size: 32px 32px;
    }
	.playEntry {
        background-position: center top;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: right;
        border: 0;
        width: 32px;
        height: 32px;
        background-size: 32px 32px;
//		padding-left: 50px;
		padding-right: 5em;
    }
	.playlistEntry {
        background-position: center top;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: right;
        border: 10;
        width: 32px;
        height: 32px;
        background-size: 32px 32px;
		padding-right: 32px;
    }
	.watchedEntry {
        background-position: top top;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: right;
        border: 0;
        width: 32px;
        height: 32px;
        background-size: 24px 16px;
//		padding-left: 50px;
		padding-right: 5em;
    }

    .albumInfo {
        background-position: center center;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: left;
        border: 0;
        width: 85px;
        height: 85px;
        background-size: 87px auto;
    }
    .artistInfo {
        background-position: center center;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: left;
        border: 0;
        width: 85px;
        height: 85px;
        background-size: 87px auto;
    }
    .movieInfo {
        background-position: center center;
        background-repeat: no-repeat;
        -webkit-border-radius: 0.5em;
        float: left;
        border: 0;
        width: 85px;
        height: 120px;
        background-size: 87px auto;
    }
    h3 {
		color:white;
        font-weight: bold;
        margin-left: 100px;
		font-size: large;

		.alignleft {
			float: left;
		}

		.alignRight {
			float: right;
			margin-right: 60px;
		}
    }
    h4 {
        color: #999;
        margin-left: 100px;
//		margin-top: 50px;
		.alignleft {
			float: left;
		}

		.alignRight {
			float: right;
			margin-right: 60px;
		}
    }
}

